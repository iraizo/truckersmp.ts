{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "noImplicitAny": false,
    "removeComments": true,
    "declaration": true,
    "noEmit": true,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.promise"
    ],
    "outDir": "dist"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}